@page "/users"

@if (Users == null)
{
    <p><em>Cargando informacion...</em></p>
}
else
{
 <table class="table table-dark">
   <thead>
     <tr>
       <th>#</th>
       <th>Nombres</th>
       <th>Usuario</th>
       <th>Telefono</th>
     </tr>
   </thead>
   <tbody> @foreach (var user in Users) { <tr>
       <td>@user.Id</td>
       <td>@user.Name</td>
       <td>@user.Username</td>
       <td>@user.Phone</td>
     </tr> } </tbody>
 </table>
}